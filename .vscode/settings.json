{
    "background.enabled": true,
    "background.customImages": [
        "file://.vscode/vscode_background.jpg",
        "file://.vscode/leimu.jpg",
        "file://.vscode/lamu.jpg",
    ],
    "background.style": {
        "content":"''",
        "pointer-events":"none",
        "position":"fixed",
        "z-index":"99999",
